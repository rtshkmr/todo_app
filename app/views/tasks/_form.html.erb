<%= form_for(@task) do |f| %>
  <ul>
  <% @task.errors.full_messages.each do |error| %>
    <li><%= error %></li>
  <% end %>
  </ul>
  <p>
    <%# label creates HTML label %>
    <%= f.label :title , "WHAT U WANT TO DO BOI"%><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :body, "tell me more about it" %><br />
    <%= f.text_area :body %>
  </p>
    <p>
        <%= f.label :tag_list, "TAG THIS TASK"%><br />
        <%= f.text_field :tag_list %>
    </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>